    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-clean-css": "^3.0.3",
    "gulp-less": "^4.0.1",
    "gulp-replace": "^0.6.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-terser": "^1.1.5",
