{
  "name": "xtend-library",
  "version": "0.9.21",
  "description": "Frontend library",
  "homepage": "http://getxtend.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82@gmail.com>",
  "repository": "https://github.com/minimit/xtend-library.git",
  "keywords": [
    "css",
    "html",
    "js",
    "front-end",
    "library",
    "plugin",
    "theme"
  ],
  "main": "dist/scripts/xtend.min.js",
  "module": "src/scripts/xtend.js",
  "umd:main": "dist/scripts/xtend.min.js",
  "umd:name": "xt",
  "files": [
    "dist/**/*.*",
    "src/scripts/**/*.*",
    "src/styles/**/*.*"
  ],
  "scripts": {
    "version": "gulp version",
    "build": "gulp build",
    "build:docs": "gulp build:docs",
    "dev": "gulp watch",
    "dev:docs": "gulp watch:docs",
    "serve": "npm run serve:docs",
    "serve:docs": "bundle exec jekyll serve",
    "dep": "npm run dep:docs",
    "dep:docs": "gh-pages -d docs",
    "release": "npx semantic-release --no-ci",
    "release:dry": "npx semantic-release --dry-run",
    "lint": "eslint src/scripts src/docs/assets/scripts",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-for-of": "^7.4.4",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "clipboard": "^2.0.4",
    "eslint": "^5.16.0",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-cached": "^1.1.1",
    "gulp-clean-css": "^3.0.3",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^0.6.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-terser": "^1.1.7",
    "gulp-util": "^3.0.8",
    "semantic-release": "^15.13.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run version && npm run build:docs",
          "successCmd": "git pull"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "_config.yml",
            "CHANGELOG.md",
            "dist/**/*.*",
            "src/**/*.*",
            "src/docs/assets/**/*.*"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
