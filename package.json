{
  "name": "xtend-library",
  "version": "0.14.8",
  "description": "Frontend library",
  "homepage": "http://getxtend.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82@gmail.com>",
  "repository": "https://github.com/minimit/xtend-library.git",
  "keywords": [
    "frontend",
    "library",
    "components",
    "vanilla"
  ],
  "main": "dist/xtend.min.js",
  "module": "src/xt.js",
  "scripts": {
    "webpack": "webpack --progress --colors",
    "build": "gatsby clean && gatsby build",
    "dev": "gatsby clean && gatsby develop -p 8888",
    "serve": "yarn build && gatsby serve -p 8888",
    "deploy": "yarn build && gh-pages -d public",
    "release": "npx semantic-release --no-ci",
    "release:dry": "npx semantic-release --dry-run",
    "lint": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "@google/markerclusterer": "^2.0.8",
    "bezier-easing": "^2.1.0",
    "core-js": "^2.6.11",
    "gsap": "^3.2.0",
    "regenerator-runtime": "^0.13.3",
    "relaxed-json": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.5.8",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "babel-loader": "^8.0.6",
    "babel-preset-gatsby": "^0.2.29",
    "clipboard": "^2.0.4",
    "css-loader": "^3.4.2",
    "eslint": "7.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.0",
    "esm": "^3.2.25",
    "gatsby": "^2.19.19",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-catch-links": "^2.1.25",
    "gatsby-plugin-less": "^2.1.2",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-page-creator": "^2.1.40",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-custom-blocks": "^2.1.25",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.6",
    "less-loader": "^6.1.0",
    "lodash.kebabcase": "^4.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "prettier-eslint": "^9.0.1",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.1.0",
    "remark-grid-tables": "^1.7.1",
    "semantic-release": "^15.14.0",
    "terser-webpack-plugin": "^1.4.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "write": "^1.0.3"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
